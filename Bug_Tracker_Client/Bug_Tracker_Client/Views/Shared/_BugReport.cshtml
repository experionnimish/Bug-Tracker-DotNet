@using BugTrackerDTOs
@model IEnumerable<BugDto>

    <table class="table table-hover table-responsive" id="dev-table">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Type</th>
                <th>Severity</th>
                <th>Priority</th>
                <th>Project</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @{
                if (Model != null)
                {
                    foreach (var item in Model)
                    {
                        <tr>
                            <td class="bug_id">item.bug_id</td>
                            <td>item.bug_name</td>
                            <td>item.bug_type</td>
                            <td>item.bug_severity</td>
                            <td>item.bug_priority</td>
                            <td><%= resultAdmin[i].projectName %></td>
                            <td class="text-center">
                                <button type="button" class="btn btn-sm btn-success" data-toggle="modal" data-target="#exampleModal" data-bug="<%= resultAdmin[i].bugID %>">View More</button>
                            </td>
                        </tr>
                    }
                }
            }
        </tbody>
    </table>
